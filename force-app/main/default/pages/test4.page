<apex:page standardController="Account">
<apex:form >
 <apex:pageBlock title="NewAccount" helpTitle="Need-Help" 
 helpUrl="https://student-fd-dev-ed.my.salesforce.com/apexpages/apexcomponents.apexp" >
 <apex:pageBlockSection columns="2" >
 <apex:inputField value="{!Account.Name}"/>
 <apex:inputField value="{!Account.Phone}"/>        
 <apex:inputField value="{!Account.fax}"/>
 <apex:inputField value="{!Account.Industry}"/>
 <apex:inputField value="{!Account.description}"/>
 <apex:inputField value="{!Account.Rating}"/>
 <apex:inputField value="{!Account.Billingcity}"/>
 <apex:inputField value="{!Account.Billingcountry}"/>
 <apex:inputHidden value="{!Account.BillingCity}"/>
<apex:pageBlockSectionItem >
  <apex:commandButton value="Save" action="{!save}" />
  <apex:commandButton value="Cancel" action="{!cancel}" disabled="true" />
</apex:pageBlockSectionItem>

 </apex:pageBlockSection>
 </apex:pageBlock>
 </apex:form>
</apex:page>